pipeline{
  agent any
    stages{
      stage("cleanws"){
        steps{
          cleanWs()
        }
      }
      stage("checkout"){
        steps{
         git branch: 'main', url: 'https://github.com/vasudevas9030/build.git'
        }
      }
      stage("build"){
        steps{
        sh "mvn clean install"
        }
      }
  }
}

